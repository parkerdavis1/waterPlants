{
	"name": "waterplants",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node build",
		"dev": "vite dev",
		"build": "vite build",
		"build:docker": "vite build --debug",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:studio": "npx drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx src/db/migrate.ts",
		"db:seed": "tsx src/db/seed.ts",
		"db:resetPre": "touch src/db/migrations/something.txt && rm -rf src/db/migrations/* && rm -f src/db/local.db",
		"db:resetPost": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:reset": "pnpm db:resetPre && pnpm db:resetPost"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.6.0",
		"@flydotio/dockerfile": "^0.7.9",
		"@internationalized/date": "^3.7.0",
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"bits-ui": "1.3.14",
		"cross-env": "^7.0.3",
		"daisyui": "^4.12.22",
		"drizzle-kit": "^0.30.5",
		"embla-carousel-svelte": "^8.5.2",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.4.0",
		"formsnap": "^2.0.0",
		"globals": "^16.0.0",
		"lucia": "^3.2.2",
		"lucide-svelte": "^0.485.0",
		"mode-watcher": "^0.5.1",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "5.25.3",
		"svelte-check": "^4.1.5",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.0",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.28.0",
		"vaul-svelte": "1.0.0-next.3",
		"vite": "^6.2.3",
		"vitest": "^3.0.9",
		"zod": "^3.24.2"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.777.0",
		"@libsql/client": "^0.15.1",
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@vitejs/plugin-basic-ssl": "^2.0.0",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.40.0",
		"tailwind-merge": "^2.5.5",
		"tailwind-variants": "^0.3.0",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.3",
		"ulid": "^3.0.0"
	}
}
