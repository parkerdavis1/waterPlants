{
	"name": "waterplants",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node --env-file=.env build",
		"dev": "vite dev",
		"build": "vite build",
		"build:docker": "vite build --debug",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:studio": "npx drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx src/db/migrate.ts",
		"db:seed": "tsx src/db/seed.ts",
		"db:resetPre": "touch src/db/migrations/something.txt && rm -rf src/db/migrations/* && rm -f src/db/local.db",
		"db:resetPost": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:reset": "pnpm db:resetPre && pnpm db:resetPost"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.9.0",
		"@flydotio/dockerfile": "^0.7.10",
		"@internationalized/date": "^3.8.2",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-netlify": "^5.1.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/kit": "^2.25.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/typography": "^0.5.16",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"bits-ui": "1.3.19",
		"cross-env": "^7.0.3",
		"daisyui": "^4.12.24",
		"drizzle-kit": "^0.30.6",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"formsnap": "^2.0.1",
		"globals": "^16.3.0",
		"lucia": "^3.2.2",
		"lucide-svelte": "^0.487.0",
		"mode-watcher": "^0.5.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "5.26.2",
		"svelte-check": "^4.3.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.37.0",
		"vaul-svelte": "1.0.0-next.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.4",
		"zod": "^3.25.76"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.848.0",
		"@libsql/client": "^0.15.10",
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@vitejs/plugin-basic-ssl": "^2.1.0",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.6.1",
		"drizzle-orm": "^0.41.0",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.20.3",
		"ulid": "^3.0.1"
	}
}
