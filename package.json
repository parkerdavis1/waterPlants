{
	"name": "waterplants",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:studio": "npx drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx src/db/migrate.ts",
		"db:seed": "tsx src/db/seed.ts",
		"db:resetPre": "touch src/db/migrations/something.txt && rm -rf src/db/migrations/* && rm -f src/db/local.db",
		"db:resetPost": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:reset": "pnpm db:resetPre && pnpm db:resetPost"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.46.0",
		"@sveltejs/adapter-auto": "^3.2.3",
		"@sveltejs/adapter-node": "^5.2.1",
		"@sveltejs/kit": "^2.5.21",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/typography": "^0.5.14",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"daisyui": "^4.12.10",
		"drizzle-kit": "^0.24.0",
		"eslint": "^9.9.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "5.0.0-next.212",
		"svelte-check": "^3.8.5",
		"sveltekit-superforms": "^2.16.1",
		"tailwindcss": "^3.4.9",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.1",
		"vite": "^5.4.0",
		"vitest": "^2.0.5",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.627.0",
		"@libsql/client": "^0.9.0",
		"@shoelace-style/shoelace": "^2.16.0",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.33.0",
		"embla-carousel-svelte": "^8.1.8",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.427.0",
		"mode-watcher": "^0.4.1",
		"svelte-sonner": "^0.3.27",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"tsx": "^4.17.0",
		"ulid": "^2.3.0",
		"vaul-svelte": "^0.3.2"
	}
}
