{
	"name": "waterplants",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node build",
		"dev": "vite dev",
		"build": "vite build",
		"build:docker": "vite build --debug",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:studio": "npx drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx src/db/migrate.ts",
		"db:seed": "tsx src/db/seed.ts",
		"db:resetPre": "touch src/db/migrations/something.txt && rm -rf src/db/migrations/* && rm -f src/db/local.db",
		"db:resetPost": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:reset": "pnpm db:resetPre && pnpm db:resetPost"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.2.0",
		"@flydotio/dockerfile": "^0.5.9",
		"@playwright/test": "^1.49.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.8.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.21.16",
		"cross-env": "^7.0.3",
		"daisyui": "^4.12.14",
		"drizzle-kit": "^0.28.1",
		"eslint": "^9.15.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"lucia": "^3.2.2",
		"lucide-svelte": "^0.460.1",
		"postcss": "^8.4.49",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "5.2.7",
		"svelte-check": "^4.1.0",
		"sveltekit-superforms": "^2.20.1",
		"tailwindcss": "^3.4.15",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.15.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.5",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.699.0",
		"@libsql/client": "^0.14.0",
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"@node-rs/argon2": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.36.4",
		"embla-carousel-svelte": "^8.5.1",
		"formsnap": "^1.0.1",
		"mode-watcher": "^0.5.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.3.0",
		"tsx": "^4.19.2",
		"ulid": "^2.3.0",
		"vaul-svelte": "^0.3.2"
	}
}
