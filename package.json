{
	"name": "waterplants",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:studio": "npx drizzle-kit studio",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "tsx src/db/migrate.ts",
		"db:seed": "tsx src/db/seed.ts",
		"db:resetPre": "touch src/db/migrations/something.txt && rm -rf src/db/migrations/* && rm -f src/db/local.db",
		"db:resetPost": "pnpm db:generate && pnpm db:migrate && pnpm db:seed",
		"db:reset": "pnpm db:resetPre && pnpm db:resetPost"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.7",
		"autoprefixer": "^10.4.19",
		"cross-env": "^7.0.3",
		"daisyui": "^4.12.10",
		"drizzle-kit": "^0.22.8",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.4",
		"svelte": "4.2.18",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^2.15.2",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vitest": "^1.2.0",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.613.0",
		"@libsql/client": "^0.7.0",
		"@shoelace-style/shoelace": "^2.15.1",
		"bits-ui": "^0.21.11",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.31.4",
		"embla-carousel-svelte": "^8.1.5",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.399.0",
		"mode-watcher": "^0.3.1",
		"svelte-sonner": "^0.3.24",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"tsx": "^4.16.2",
		"ulid": "^2.3.0",
		"vaul-svelte": "^0.3.1"
	}
}
